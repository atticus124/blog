<h1>Sign-up</h1>


<div class = "container">
  <div class="row">

    <%= bootstrap_form_for(@user) do |f| %>
    
    <%= f.text_field :username, label: "Username" %>
    
    <%= f.password_field :password, label: "Password" %>
    
    <%= f.password_field :password_confirmation, label: "Confirm Password" %>
    
    <%= f.submit "Sign Up"%>
    <% end %>

  </div>
</div>
